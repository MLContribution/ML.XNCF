    Layout = "_XscfModuleDocsContainerLayout";
}
@{
    ViewData["Title"] = "显示MarkDownContainer的文档演示";
}

@section Style{
    
    <link href="~/css/Admin/docs.css" rel="stylesheet" />
}

<div class="header">
    <div class="logo"><img src="https://weixin.senparc.com/images/SCF/logo.png" width="100" /></div>
</div>
<div id="menu" class="menu"></div>
<div id="anchor" class="anchor"></div>
<div id="chapter-list" class="chapter-list"></div>
<div id="content" class="content">
    <div id="readme" class="readme markdown-body"></div>
</div>
<div class="zoom-img">
